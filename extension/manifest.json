    }
  ],

  "web_accessible_resources": [
    "bower_components/jquery/dist/jquery.min.map"
  ],

  "icons": { 
    // "16": "icon16.png",
    // "48": "icon48.png",
    // "128": "icon128.png" 
  }
}
